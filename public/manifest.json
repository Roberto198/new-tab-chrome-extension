{
  "name": "React Chrome NewTab Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-3p9pEyA9xRQBW+aZswAPaaTGoBvf2KdXkMcdctSJFU8='; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "history"
  ]
}
